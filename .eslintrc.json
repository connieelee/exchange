{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "chrome": true
  },
  "rules": {
    "class-methods-use-this": 0,
    "arrow-parens": 0,
    "react/jsx-filename-extension": 0,
    "jsx-a11y/anchor-is-valid": [0, { "components": [ "Link" ] }]
  }
}
