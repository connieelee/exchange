{
  "manifest_version": 2,
  "name": "Exchange",
  "description": "An extension that displays monetary values in an exchange rate determined by items deemed valuable to the user.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "exchange"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_security_policy": "script-src 'self' http://localhost:8097; object-src 'self'"
}